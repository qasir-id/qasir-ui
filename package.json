{
  "name": "qasir-ui-dev",
  "version": "1.0.12",
  "private": false,
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "autosuggest-highlight": "^3.1.1",
    "clsx": "latest",
    "dayjs": "^1.8.29",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "npx http-server ./storybook-static",
    "chromatic": "npx chromatic --project-token=nnkmnd7wfvm",
    "build:variables": "rm -rf variables && mkdir variables && babel src/components/variables -d variables --copy-files",
    "build:layout": "rm -rf layout && mkdir layout && babel src/components/layout -d layout --copy-files",
    "build:inputs": "rm -rf inputs && mkdir inputs && babel src/components/inputs -d inputs --copy-files",
    "build:navigation": "rm -rf navigation && mkdir navigation && babel src/components/navigation -d navigation --copy-files",
    "build:data-display": "rm -rf data-display && mkdir data-display && babel src/components/data-display -d data-display --copy-files",
    "build": "npm-run-all build:variables build:layout build:inputs build:navigation build:data-display"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.12.11",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.12.10",
    "@etchteam/storybook-addon-status": "^2.1.0",
    "@storybook/addon-a11y": "^6.1.12",
    "@storybook/addon-actions": "^6.1.12",
    "@storybook/addon-controls": "^6.1.12",
    "@storybook/addon-design-assets": "^6.1.12",
    "@storybook/addon-docs": "^6.1.12",
    "@storybook/addon-viewport": "^6.1.12",
    "@storybook/addons": "^6.1.12",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.12",
    "@storybook/theming": "^6.1.12",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.0.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-is": "^16.13.1",
    "storybook-addon-designs": "^5.4.2",
    "storybook-addon-performance": "^0.13.0",
    "ts-loader": "^8.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  }
}
